{% extends 'layout-2columns.twig' %}
{% block stylesheet %}
    {#<link href="{{ base_url }}/assets/css/xxxxx.css" rel="stylesheet">#}
    <style>
        .rule-book {
            margin-bottom: 24px;
        }

        .rule-book .rule-book-header {
            margin: 0 0 0 0;
        }

        .rule-book .rule-book-header h3 {
            margin: 0;
            padding: 20px 0 20px 0;
        }

    </style>
{% endblock %}
{% block javascript %}
    <script src="{{ base_url }}/assets/js/jquery.smooth-scroll.min.js"></script>
    <script>
        $('a[href^="#"]').smoothScroll({
            offset: -70,
        });
        // 別ページからページ内アンカー付きで遷移してきた場合
        var ruleUniqueId = window.location.href.slice(window.location.href.indexOf('#'));
        if (ruleUniqueId != '/') {
            $('a[href^="' + ruleUniqueId + '"]').trigger('click');
        }
    </script>
{% endblock %}
